- content_for :google_maps_api do
  = javascript_include_tag 'https://maps.googleapis.com/maps/api/js?key=AIzaSyCANOejxMMuIDDlIhtx8QoXa7VHW6LU5t0'

  :javascript
    function initialize() {
      var mapOptions = {
        center: new google.maps.LatLng(1.362497, 103.804148),
        zoom: 12
      };
      var map = new google.maps.Map(document.getElementById("map-canvas"), mapOptions);
    }

    google.maps.event.addDomListener(window, 'load', initialize);

#map-canvas
